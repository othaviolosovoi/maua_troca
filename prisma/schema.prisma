generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["driverAdapters"]
}

datasource db {
  provider  = "postgresql"
  url       = env("POSTGRES_URL")
  directUrl = env("POSTGRES_URL_NON_POOLING")
}

model User {
  id      String      @id @default(cuid())
  name    String
  email   String   @unique
  password String
  currentRoom String
  desiredRoom String
}