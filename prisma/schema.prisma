generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["driverAdapters"]
}

datasource db {
  provider  = "postgresql"
  url       = env("POSTGRES_PRISMA_URL")
}
model User {
  id      String      @id @default(cuid())
  name    String
  email   String   @unique
  password String
  currentRoom Int
  desiredRoom Int
}